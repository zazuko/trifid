  "devDependencies": {
    "@rdfjs/types": "^1.1.0",
    "@types/node": "^20.10.6",
    "c8": "^9.1.0",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.3.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
