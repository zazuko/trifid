  "main": "index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "trifid-core": "./server.js"
  },
  "dependencies": {
    "@fastify/accepts": "^4.3.0",
    "@fastify/cookie": "^9.3.1",
